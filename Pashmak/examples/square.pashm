# Print an square

func print_square($args)
    $args = format_args($args)
    $x = $args[0]
    $y = $args[1]
    $i = 0
    while $i < $x
        $j = 0
        while $j < $y
            print('*')
            $j = $j + 1
        endwhile
        println('')
        $i = $i + 1
    endwhile
endfunc

print_square(10, 25)
